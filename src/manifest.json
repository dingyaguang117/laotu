
{
	"name": "老兔网站分析",
	"manifest_version": 2,
	"version": "0.2",
	"permissions":
	[
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"browser_action":
	{
		"name": "Show Website Info",
		"default_title":"title....",
		"default_badge":"badg",
		"default_icon":"img/icon.png",
        "default_popup": "popup.html"
	},
  	"content_security_policy": "script-src  'self' 'unsafe-eval'; object-src 'self'"
}